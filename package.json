{
  "name": "attendance",
  "private": true,
  "scripts": {
    "build": "turbo run build --parallel",
    "dev": "cross-env NODE_ENV=development turbo run dev --parallel",
    "start": "cross-env NODE_ENV=production turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "zod": "^4.0.17"
  }
}
